    <% if(sponsors.length) { %>

        <div class="container">
            <h1>These are people whom I am thankful</h1>
        </div>

        <div class="container">
            <ul>
                <% for(var i = 0; i < sponsors.length; i ++) { %>
                    <li>
                        <% if(sponsors[i].url) { %>
                            <a href="<%- sponsors[i].url %>"><%- sponsors[i].name %></a>
                        <% } else { %>
                            <span><%- sponsors[i].name %></span>
                        <% } %>
                    </li>
                <% } %>
            </ul>
        </div>

    <% } else { %>
        <div class="container">
            <h1>Be first :)</h1>
        </div>
    <% } %>


